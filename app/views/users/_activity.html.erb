<ul class="collection">


  <% @created.each do |event| %>



    <li class="collection-item avatar">

      <% if event.title %>
        <img src="images/yuna.jpg" alt="" class="circle">
      <% else %>
        <i class="material-icons circle">face</i>
      <%end %>

    <p>
      <%= event.title %>
    </p>
    <p>
      <font class="messagedate">
        Posted by <a href="/users/"></a> on <%= event.created_at.strftime("%A, %B %-d at %l:%M %p") %>
      </font>
    </p>

     </li>

  <% end %>


</ul>